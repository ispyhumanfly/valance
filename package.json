    "path-exists": "^3.0.0",
    "posthtml": "^0.9.1",
    "posthtml-pug": "^1.0.1",
    "posthtml-web-component": "^0.1.0",
    "prettyjson": "^1.2.1",
    "pug": "^2.0.0-beta6",
    "pug-beautifier": "0.0.1",
    "pug-filters": "^2.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "recursive-search": "^1.0.1",
    "redis": "^2.6.2",
    "reflect-metadata": "^0.1.8",
    "require": "^2.4.20",
    "requirejs": "^2.3.3",
    "restify": "^4.2.0",
