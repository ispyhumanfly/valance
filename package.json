    "typedoc": "^0.5.9",
    "typescript": "^2.0.6",
    "typings": "^2.1.0",
    "uglify": "^0.1.5",
    "uglify-js": "^2.7.4",
    "watch": "^1.0.1",
    "webcomponents.js": "^0.7.24",
    "x-tag": "^1.5.11",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "shelljs-release": "^0.2.0",
    "tslint-config-standard": "^4.0.0",
    "typescript-compiler": "^1.4.1-2",
    "ts-dependency-injection": "^1.2.2",
    "ts-node": "^3.0.2"
  }
}
